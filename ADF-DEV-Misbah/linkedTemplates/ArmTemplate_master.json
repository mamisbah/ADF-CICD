{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-DEV-Misbah"
		},
		"LSdevAzureBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LSdevAzureBlobStorage'"
		},
		"SqlServerLS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SqlServerLS'"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://KV-Dev-Misbah.vault.azure.net/"
		},
		"AzureKeyVault2_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://KV-Dev-Misbah.vault.azure.net/"
		},
		"RestService1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://pokeapi.co/api/v2/pokemon"
		},
		"SqlServerLS_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "adfmssqlserver.database.windows.net"
		},
		"SqlServerLS_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "adf_test_db"
		},
		"SqlServerLS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "mamisbah"
		},
		"RestService2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://pokeapi.co/api/v2/pokemon"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-DEV-Misbah_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LSdevAzureBlobStorage_connectionString": {
						"value": "[parameters('LSdevAzureBlobStorage_connectionString')]"
					},
					"SqlServerLS_password": {
						"value": "[parameters('SqlServerLS_password')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"AzureKeyVault2_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault2_properties_typeProperties_baseUrl')]"
					},
					"RestService1_properties_typeProperties_url": {
						"value": "[parameters('RestService1_properties_typeProperties_url')]"
					},
					"SqlServerLS_properties_typeProperties_server": {
						"value": "[parameters('SqlServerLS_properties_typeProperties_server')]"
					},
					"SqlServerLS_properties_typeProperties_database": {
						"value": "[parameters('SqlServerLS_properties_typeProperties_database')]"
					},
					"SqlServerLS_properties_typeProperties_userName": {
						"value": "[parameters('SqlServerLS_properties_typeProperties_userName')]"
					},
					"RestService2_properties_typeProperties_url": {
						"value": "[parameters('RestService2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}